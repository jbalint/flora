	    attach_workspace(WS,
			     head_neg_struct(PosCode,Idx,NegCode,Status1),
			     NegCode,Code,WSStatus),
	    (WSStatus == NO_ERRORS -> append(S,Status1,Status)
	    ; append(Status1,WSStatus,Status)
	    )
	; Status = S
	),
	!.


