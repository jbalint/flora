	    attach_workspace(WS,
			     objeql_struct(HEADLITERAL,O1,O2,OECode),
			     OECode,WSCode,WSStatus),
	    conjunct_struct(OCode,WSCode,Code)
	;
	    WSStatus = NO_ERRORS
	),
	(WSStatus == NO_ERRORS -> Status = PathexpStatus
	; Status = WSStatus
	),
