	( no_errors_found(PStatus) -> 
	    compile_pathexp(Hilog,TOP_CALL,HCode,_HOidCode,_HObjSpec,[],Status),
	    ( no_errors_found(Status) -> 
		p2h_struct(PCode,HCode,Code)
