	    %%is_flatom(Funct,FAtom), %% this prevents f(_p(a))@_plg(mod) from getting compiled
	    compile_prlgterm(Funct,N,Args,Module,
			     Nesting,
			     Object,OidCode,Code,BodyHeadList,Status)
	;
	    %%is_flatom(ParserTerm,FAtom), %% this prevents f(_p)@_plg(mod) from getting compiled
	    atomobj_struct(ParserTerm,FObj),
	    N=0,
	    prologterm_struct(FObj,0,[],Module,Object),
	    Code=NULL,
	    OidCode=NULL,
	    BodyHeadList=[],
