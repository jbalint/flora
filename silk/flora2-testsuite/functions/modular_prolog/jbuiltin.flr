:- use_argumentation_theory{ogcle} .
?- [ogclpe>>ogcle] .

silk#jr(ovr2(), t) :- a(?)@main.

silk#ja(${_overrides(e, b)@main}, f, ovr2()) :- not _overrides(e, b)@main and silk#jr(ovr2(), f)@builtin .
silk#ja(${_overrides(e, b)@main}, t, ovr2()) :- _overrides(e, b)@main and silk#jr(ovr2(), t)@builtin .

:- prolog foo/1.

ppppppp(${_overrides(e,b)@main}) :- _overrides(e, b)@main, foo(?)@main.


%jbuiltin_test :- foo(?)@main, writeln(tst2=bbbbbb)@_plg.


