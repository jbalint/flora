
#include "modular_prolog/jmain.flr"
?- [+'modular_prolog/jbuiltin'>>builtin].


%tst1 :- silk#ja(${_overrides(e, b)@main}, t, ovr2())@builtin,
	writeln(tst1=ok)@_plg.

%tst2 :- %jbuiltin_test@builtin.

%tst3 :- %jmain_test.

%pretest :- %tst1, %tst2, %tst3.

%test :- tell(temp)@_plg,
	%pretest,
	told@_plg.
