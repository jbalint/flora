dd(bb).
isA(a,b).
subclassOf(b,c).
@{a} (?x : ?c) :- ?x(?c).
@{?} (?x : ?c) :- ?x(?c).
@{?} ?x : ?c :- ?x(?c).
@{?, silk#foo} isA(?x,?c2) :- isA(?x,?c1) and subclassOf(?c1,?c2).

%test1 :- ?x:?c, ?c != _object, _isatom(?c), writeln(test1=?x+?c)@_plg.
%test2 :- isA(a,c), writeln(test2=ok)@_plg.

%pretest :- %test1, %test2.

%test :- tell(temp)@_plg,
	%pretest,
	told@_plg.
