%test :- [basetype_moo>>moo],
	[basetype_foo>>foo],
	tell(temp)@_plg(),
	Method[%mustDefine(on)]@_sys,
	%pretest,
	!,
	Method[%mustDefine(off)]@_sys,
	Tables[%abolish]@_sys,
	%pretest,
	told@_plg().
