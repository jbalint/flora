pppp :- true.

%tst1 :-
	write('test1.1: ')@_plg,
	(not(writeln(aaaaa)@_plg) ; true),
	incr_flora_update_counter@_plg(flrnegation),
	write('test1.2: ')@_plg,
	(not(writeln(aaaaa)@_plg) ; true).

%tst2 :-
	write('test2: ')@_plg,
	(true{aaaaaa} ; write(aaaaaa1)@_plg),
	insert{aaaaaa},
	(false{aaaaaa} ; writeln(aaaaaa2)@_plg).

%pretest :- %tst1, %tst2, delete{aaaaaa}.


%test :- tell(temp)@_plg(),
        Method[%mustDefine(on)]@_sys,
        %pretest,
        Method[%mustDefine(off)]@_sys,
        %pretest,
        told@_plg().

