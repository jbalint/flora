
?- ['programs/Grade' >> grade], ['programs/Class' >> class].


%pretest :-
	%full_report([midterm,proj1,proj2,final])@grade,
	%id_scorelist_report([midterm,proj1,proj2,final])@grade.

%test :- tell(temp)@_plg(),
	Method[%mustDefine(on)]@_sys,
	%pretest,
	Method[%mustDefine(off)]@_sys,
	%pretest,
	told@_plg().


