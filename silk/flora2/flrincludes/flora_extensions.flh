/*  File extensions used in FLORA-2 */

#define FLORA_FILE_EXT		flr
#define FLORA_INCLUDE_EXT	fli
#define FLORA_HEADER_EXT	flh
#define FLORA_FDB_EXT		fdb

/* Extension for storage of defined predicate skeletons */
#define FLORA_FLD_EXT		fld

/* Extension for storage of signature constraints checking predicates */
#define FLORA_CON_EXT		con

/* Extension for aux files used to record symbol that occur in FLORA-2 KBs */
#define FLORA_FLS_EXT		fls

/* Extension for files containing tabling directives for predicates generated
   for user defined functions */
#define FLORA_FLT_EXT		flt
