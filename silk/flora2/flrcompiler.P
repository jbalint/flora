	flora_new_varobj(Index,Val), 
        ( is_mvdrefop(RefType) -> 	 
            mvdspec_struct(HEADLITERAL,Obj,Att,Val,Code) 	 
        ; 	 
          is_imvdrefop(RefType), 	 
          imvdspec_struct(HEADLITERAL,Obj,Att,Val,Code) 	 
