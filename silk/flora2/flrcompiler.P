	is_flpassthru(SpecTerm,P),
	is_flterm(P, Funct, N, Args),
	!,
	compile_prlgterm(Funct,N,Args,TCode,_OidCode,ACode,Status),
	(Status == [] -> conjunct_code([ACode,TCode],Code); true).

