%% transactional numbers are used only in :- index %arity-argument
transactionalnumobj_struct(FLTRANSACTIONALNUMBER(Number,I),PRTRANSACTIONALNUMBER(Number,I)).

varobj_struct(FLVAR(Name,Index),PRVARIABLE(Name,Index)).
