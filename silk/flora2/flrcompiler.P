	( is_mvdrefop(RefType) ->
	    mvddef_struct(LiteralContext,Obj,Att,Code)
	;
	  is_imvdrefop(RefType),
	  imvddef_struct(LiteralContext,Obj,Att,Code)
        ).
*/

