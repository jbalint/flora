	compile_body(Formula,FCode,Status),
	(Status == []
	-> truthvalue_struct(FCode,OpCode,Code)
	; true
	).


/****************************************************************************
   compile_labelprimitive(+LabelParserTerm,?HeadList,-Code,-Status)
