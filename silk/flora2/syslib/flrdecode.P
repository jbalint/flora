	(Level==0 -> ReifyStart='',ReifyEnd='' ; ReifyStart='${',ReifyEnd='}'),
	Depth1 is Depth+1,
	(var(Unwrapped) -> UnwrappedCode='_Var'
	; flora_decode_goal(Unwrapped,UnwrappedCode,Depth1)
	).

