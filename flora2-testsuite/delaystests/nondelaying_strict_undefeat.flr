:- use_argumentation_theory.

(?x : ?c) :- ?c(?x)@main .
neg ?c(?x) :- neg (?x : ?c) .
?c(?x) :- (?x : ?c) .
neg (?x : ?c) :- neg ?c(?x)@main .

neg ?_sub(?x) :- neg ?super(?x)@main .
?_super(?x) :- ?sub(?x)@main .

%tst1 :-
	write('test1: ')@_plg,
	?query : silk#PersistentQuery,
	writeln(ok)@_plg.

%pretest :- %tst1.

%test :- tell(temp)@_plg(), 
	%pretest,
	told@_plg().
