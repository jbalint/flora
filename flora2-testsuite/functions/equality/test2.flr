:- prolog fib/2.
:- table fib/2.

fib(0,1) :- !.
fib(1,1) :- !.
fib(?N,?R) :-
       is(?N1,?N-1)@_plg, is(?N2,?N1-1)@_plg,
       fib(?N1,?R1), fib(?N2,?R2), is(?R,?R1+?R2)@_plg.

test(?X,?Y) :- cputime(?T)@_plg, fib(?X,?Y), cputime(?TT)@_plg,
       is(?TTT,?TT-?T)@_plg, writeln(cputime=?TTT)@_plg.

?- nl@_plg, test(10000,?Y).
