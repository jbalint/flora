udf foo(?x):=?y if abc(?x,?y).
udf moo(?x):=?y if xyz(?x,?y).
//foo(a) ~> b.
abc(a,b).
//foo(b) ~> c.
abc(b,c).

p(g(c)).

//moo(c) ~> b
xyz(c,b).

f(g(moo(c))).

%test1:-p(g(foo(foo(a)))),writeln('yes')@_plg,!.
%test1:-writeln('no')@_plg.
%test2:-p(g(foo(?x))),writeln(?x)@_plg.
%test3:-f(g(foo(?x))),writeln(?x)@_plg.

%pretest :- %test1, %test2, %test3.

%test:-
	tell(temp)@_plg,
	%pretest,
	told@_plg.

