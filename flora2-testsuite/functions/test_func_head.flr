function teach(?x) := ?y if teach(?x,?y).
teach_cs(teach(?x)):-class(?x),cs(?x).
//teach_cs2(?y):-teach(?x,?y),class(?x),cs(?x).
cs(theory).
class(theory).
teach(theory,bob).

%test1 :- teach_cs(?y), writeln(?y)@_plg.
%test2 :- teach_cs(teach(?x)), writeln(?x)@_plg.
%test3 :- ?y=teach(theory), writeln(?y)@_plg.

%pretest :- %test1, %test2, %test3.

%test :-
	tell(temp)@_plg,
	%pretest,
	told@_plg.
