function ?x+?y:=?z if _isnumber(?x), _isnumber(?y), ?z is ?x+?y, _isnumber(?z).
function plus2(?x):=?y if _isnumber(?x), ?y=?x+2, _isnumber(?y).
function inc(?x):=?y if _isnumber(?x), ?y=?x+1, _isnumber(?y).
function inc1(?z)(?x):=?y if ?y is ?x+?z+1, _isnumber(?y).

p(5).
p(7).
p(11).
p(14).

q(2).
q(4).
q(5).

%test1 :-
	inc(4) = ?r1, _isnumber(?r1),
	%writeln(?r1)@_io,
	inc(plus2(7)) = ?r2, _isnumber(?r2),
	%writeln(?r2)@_io, fail.
%test1 :- true.

%test2 :- write('test2 = ')@_plg, q(?y), p(inc1(?y)(plus2(7))), writeln(?y)@_plg, fail.
%test2 :- true.

%test :-
	tell(temp)@_plg,
	%test1,
	told@_plg.
