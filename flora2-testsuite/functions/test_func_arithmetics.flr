function ?x+?y:=?z if ?z is ?x+?y.
function plus2(?x):=?x+2.
function inc(?x):=?y if ?y=?x+1.
function inc1(?z)(?x):=?y if ?y is ?x+?z+1.

p(5).
p(7).
p(11).
p(14).

q(2).
q(4).
q(5).

%test1 :-
	writeln(inc(4))@_plg,
	writeln(inc(plus2(7)))@_plg.

%test2 :- write('test2 = ')@_plg, q(?y), p(inc1(?y)(plus2(7))), writeln(?y)@_plg, fail.
%test2 :- true.

%test :-
	tell(temp)@_plg,
	%test1,
	told@_plg.
