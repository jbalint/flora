udf p(?x,?y):=?z if pp(?x,?y,?z).
udf f(?x,?y):=?z if ff(?x,?y,?z).

q(?a,?b):-p(?a,?b)=f(?a,?b).

pp(c,d,e).
ff(c,d,e).
pp(g,d,t).
ff(g,d,h).

test1:- q(c,d),writeln('yes')@_plg,!.
test1:- writeln('no')@_plg.
test2:- q(g,d),writeln('yes')@_plg,!.
test2:- writeln('no')@_plg.
test3:- q(?x,?y),writeln(?x)@_plg,writeln(?y)@_plg.

%test:-
	tell(temp)@_plg,
	test1,
	test2,
	test3,
	told@_plg.
