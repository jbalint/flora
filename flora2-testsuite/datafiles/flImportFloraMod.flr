
a[b->c].
e[f->h].

p(?X) :- a[b->?Y], ?HX=p(?Y,_@), p2h{?X,?HX}, caller{?CC}, writeln(caller=?CC)@_plg().
%q(?X) :- e[f->?Y], ?HX= q(?Y,_@), p2h{?X,?HX}.

%addRelationInstance(
	?RelationName,?Object1,?Object2,?StartTime,?EndTime) :-
	insert{?RelationName(?Object1,?Object2,?StartTime,?EndTime)},
	writeln('Relation instance added')@_plg().


