
?- [prettyprint>>pp].

?- ['../datafiles/pretty_data'>>data].

%test1 :- writeln('Self info: ')@_plg(), student[pp_self(data)]@pp.
%test2 :- writeln('Class info: ')@_plg(), student[pp_class(data)]@pp.
%test3 :- writeln('ISA info: ')@_plg(), student[pp_isa(data)]@pp.

%test4 :- true{?X:student[taught->->?L]@data},
	sort(?L,?SortedL)@_plg(),
	write(?X)@_plg(), write('  taught: ')@_plg(),
	writeln(?SortedL)@_plg(),
	fail.
%test4 :- true.

%test :- tell(temp)@_plg(),
	Method[mustDefine(off(data))]@_sys,
	%test1, %test2, 
	%test3,
	%test4, 
	told@_plg().


