// symmetric property
?X[prop->?Y] :- ?Y:object[prop->?X:object].
?- insert{obj1:object}.
?- insert{obj2:object}.


%tst1 :- write('tst1: ')@_plg(),
	 if ?_X[prop->?_Y] then writeln(failed)@_plg()
	 else writeln(ok)@_plg().

%tst2 :- write('tst2: ')@_plg(),
	 insert{obj1[prop->obj2]},
	 insert{obj3:othertype[prop->obj2]},
	 ?_X[prop->?_Y],
	 writeln('ok')@_plg(),
	 deleteall{?_[prop->?_], ?_:othertype}.

%pretest :- %tst1, %tst2.

%test :- tell(temp)@_plg(),
        Method[%mustDefine(on)]@_sys,
        %pretest,
        Method[%mustDefine(off)]@_sys,
        %pretest,
        told@_plg().


