p(?X)[?M*->?V] :- ?X[?M*->?V].
a[m*->v].
a[mm*->vv].

//?- _optimize(local_override).

%tst1 :- a[?M*->?V],
	(write('tst1: '), write(meth=?M), write(' '), writeln(val=?V))@_plg(),
	fail.
%tst1 :- true.

%tst2 :- p(a)[?M*->?V],
	(write('tst2: '), write(meth=?M), write(' '), writeln(val=?V))@_plg(),
	fail.
%tst2 :- true.

%pretest :- %tst1, %tst2.

%test :- tell(temp)@_plg(),
	Method[%mustDefine(on)]@_sys,
	%pretest,
	Method[%mustDefine(off)]@_sys,
	abolish_all_tables,
	%pretest,
	told@_plg().



