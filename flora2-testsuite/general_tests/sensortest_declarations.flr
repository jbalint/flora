:- defsensor( sens1(?Y,?X),sensorfoo,(nonvar(?X),ground(?Y))).
:- defsensor( sens2(?Y,?X),(ground(?X),ground(?Y))).


//sens2(?F,?L,?X,?Y) :- ?X==a, ?Y==b, !, writeln(?F+?L)@_plg.
sens2(?X,?Y) :- ?X==a, ?Y==b, !, writeln(_?F+_?L)@_plg.
//sens2(?F,?L,?X,?Y) :-
sens2(?X,?Y) :-
	if \+_isground(?X) then write('Instantiation error in sens2: ?X. ')@_plg,
	if \+_isground(?Y) then write('Instantiation error in sens2: ?Y. ')@_plg,
	write('In file ')@_plg, write(_?F)@_plg,
	write(' on line ')@_plg, write(_?L)@_plg,
	write(': ')@_plg,
	if _isvar(?X) then writeln(sensor2=?Y+' ?X unbound')@_plg,
	if _isvar(?Y) then writeln(sensor2=?X+' ?Y unbound')@_plg,
	if (_isnonvar(?X),_isnonvar(?Y)) then writeln(sensor2=?X+?Y)@_plg,
	?X = bbbbb,
	//sens2(?F,?L,a,b),
	sens2(a,b),
	!.

