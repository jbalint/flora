oa:ca.
ca::da.
da[*foo(k)].


%tst1 :- write('tst1: ')@_plg(),
	 insert{ca[*foo(u)]},
	 oa[foo(?Xa)],
	 writeln(?Xa)@_plg().
%tst2 :- write('tst2: ')@_plg(), oa[foo(?Xa)], writeln(?Xa)@_plg().
%tst3 :- write('tst3: ')@_plg(),
	 delete{ca[*foo(?_)]},
	 oa[foo(?Xa)],
	 writeln(?Xa)@_plg().
%tst4 :- write('tst4: ')@_plg(), oa[foo(?Xa)], writeln(?Xa)@_plg().

%pretest :- 
	abolish_all_tables,
	setsemantics{equality(none)},
	%tst1, %tst2,
	setsemantics{equality(basic)},
	%tst3, %tst4.


%test :- tell(temp)@_plg(),
	Method[mustDefine(on)]@_sys,
	%pretest,
	Method[mustDefine(off)]@_sys,
	%pretest,
	told@_plg().


